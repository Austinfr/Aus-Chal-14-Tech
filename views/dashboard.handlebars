<h1>Your Dashboard</h1>

<div class="dashboard-container">
  <h2>My Posts</h2>
  {{#if posts}}
    <ul class="post-list">
      {{#each posts}}
        <li>
            {{> post-template title=title postStamp=postStamp content=content id=id }}
        </li>
      {{/each}}
    </ul>
  {{else}}
    <p>You have not created any posts yet.</p>
  {{/if}}
  <a href="/newpost" class="create-post-btn">Create New Post</a>
</div>

